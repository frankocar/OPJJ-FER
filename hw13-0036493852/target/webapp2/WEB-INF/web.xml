<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
		 http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1"
  metadata-complete="false">

 <display-name>Homework13</display-name>

 <servlet>
    <servlet-name>colorServlet</servlet-name>
    <jsp-file>/WEB-INF/pages/colors.jsp</jsp-file>
  </servlet>
  
  <servlet>
    <servlet-name>trigServlet</servlet-name>
    <jsp-file>/WEB-INF/pages/trigonometry.jsp</jsp-file>
  </servlet>
  
  <servlet>
    <servlet-name>funny</servlet-name>
    <jsp-file>/WEB-INF/stories/funny.jsp</jsp-file>
  </servlet>
  
  <servlet>
    <servlet-name>usageChart</servlet-name>
    <jsp-file>/WEB-INF/pages/report.jsp</jsp-file>
  </servlet>
  
  <servlet>
    <servlet-name>tableError</servlet-name>
    <jsp-file>/WEB-INF/pages/tableError.jsp</jsp-file>
  </servlet>
  
  <servlet>
    <servlet-name>appInfo</servlet-name>
    <jsp-file>/WEB-INF/pages/appinfo.jsp</jsp-file>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>colorServlet</servlet-name>
    <url-pattern>/colors</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>trigServlet</servlet-name>
    <url-pattern>/trig</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>funny</servlet-name>
    <url-pattern>/funny</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>usageChart</servlet-name>
    <url-pattern>/usageChart</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>tableError</servlet-name>
    <url-pattern>/tableError</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>appInfo</servlet-name>
    <url-pattern>/appinfo</url-pattern>
  </servlet-mapping>


</web-app>